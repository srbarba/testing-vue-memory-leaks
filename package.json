{
  "name": "testing-vue-memory-leaks",
  "version": "1.0.0",
  "author": "JuanP Barba <jpbs92@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --selectProjects test-utils testing-library with-setup",
    "test:cov": "jest --collect-coverage",
    "test:leaks": "yarn test --runInBand --no-cache --detectLeaks --logHeapUsage",
    "test-utils": "yarn jest --selectProjects test-utils",
    "tu:leaks": "yarn test-utils --runInBand --no-cache --detectLeaks --logHeapUsage",
    "testing-library": "yarn jest --selectProjects testing-library",
    "tl:leaks": "yarn testing-library --runInBand --no-cache --detectLeaks --logHeapUsage",
    "with-setup": "yarn jest --selectProjects with-setup",
    "ws:leaks": "yarn with-setup --runInBand --no-cache --detectLeaks --logHeapUsage"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/vue": "^5.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "typescript": "^4.2.2",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12",
    "weak-napi": "^2.0.2"
  },
  "dependencies": {
    "@types/chance": "^1.1.1",
    "chance": "^1.1.7",
    "vue-i18n": "^8.23.0",
    "vuetify": "^2.4.5"
  }
}
